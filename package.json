{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node server/server",
    "dev": "nodemon",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build:client": "tsc && vite build",
    "build": "npm run build:client && npm run build:server"
  },
  "dependencies": {
    "express": "^4.17.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "preact": "^10.5.15",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "alias-hq": "^5.3.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
