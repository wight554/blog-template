{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "cleanup": "rimraf dist",
    "dev": "nodemon",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "tsc -p src/tsconfig.json && vite build",
    "build": "yarn cleanup && yarn build:client && yarn build:server && yarn postbuild",
    "postbuild": "node ./scripts/postbuild.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/static": "^6.5.0",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@5.10.6",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "final-form": "^4.20.7",
    "htm": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.6.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preact": "^10.11.0",
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest",
    "react-final-form": "^6.5.9",
    "react-router-dom": "^6.4.1",
    "recoil": "^0.7.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "styled-components": "^5.3.6",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.1.2",
    "@preact/preset-vite": "^2.4.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/preact": "^3.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/node": "^18.7.23",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitest/coverage-istanbul": "^0.23.4",
    "eslint": "^8.24.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.7.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "typescript-lit-html-plugin": "^0.9.0",
    "typescript-styled-plugin": "^0.18.2",
    "vite": "^3.1.4",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "0.23.4"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@5.10.6"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#server/*": "./server/*",
    "#test/*": "./test/*"
  }
}
