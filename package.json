{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node server/server",
    "dev": "nodemon",
    "prod": "cross-env NODE_ENV=production && node ./dist/server/server.js",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc && vite build",
    "build": "npm run build:client && npm run build:server"
  },
  "dependencies": {
    "express": "^4.17.2",
    "preact": "^10.5.15"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
