{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only server/server",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json  && vite build",
    "build": "yarn run build:client && yarn run build:server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn run test -- --coverage",
    "lint": "eslint \"{src,server}/**/*.{ts,tsx}\"",
    "lint:fix": "yarn run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "mongoose": "^6.2.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preact": "^10.5.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.6",
    "@preact/preset-vite": "^2.1.5",
    "@testing-library/preact": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sinon": "^10.0.10",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "alias-hq": "^5.3.2",
    "c8": "^7.11.0",
    "eslint": "^8.8.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2",
    "vitest": "^0.2.7"
  },
  "packageManager": "yarn@3.1.1"
}
