{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "cleanup": "rimraf dist",
    "dev": "nodemon",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p server/tsconfig.json && tsc-alias --dir dist",
    "build:client": "tsc -p src/tsconfig.json && vite build",
    "build": "yarn cleanup && yarn build:client && yarn build:server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "postinstall": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "dependencies": {
    "@fastify/cookie": "^6.0.0",
    "@fastify/static": "^5.0.2",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.8.0",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mongoose": "^9.1.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "final-form": "^4.20.7",
    "htm": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preact": "^10.7.2",
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest",
    "react-final-form": "^6.5.9",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.5",
    "@preact/preset-vite": "^2.2.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/preact": "3.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/node": "^17.0.36",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "c8": "^7.11.3",
    "eslint": "^8.16.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "tsc-alias": "^1.6.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "typescript-lit-html-plugin": "^0.9.0",
    "typescript-styled-plugin": "^0.18.2",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-typescript": "^1.0.4",
    "vitest": "^0.12.9"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#server/*": "./server/*",
    "#test/*": "./test/*"
  }
}
