{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "cleanup": "rimraf dist",
    "dev": "tsnd --respawn server/server",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json && vite build",
    "build": "yarn cleanup && yarn build:client && yarn build:server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint \"{src,server,test}/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\" \"test/**/*.{ts,tsx}\"",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.3.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-cookie": "^5.5.0",
    "fastify-static": "^4.5.0",
    "mongoose": "^6.2.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preact": "^10.6.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@nestjs/testing": "^8.3.1",
    "@preact/preset-vite": "^2.1.7",
    "@swc/core": "^1.2.142",
    "@testing-library/preact": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/node": "^17.0.18",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "alias-hq": "^5.3.2",
    "c8": "^7.11.0",
    "eslint": "^8.9.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-mockito": "^2.6.1",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.0",
    "typescript": "^4.5.5",
    "unplugin-swc": "^1.1.3",
    "vite": "^2.8.4",
    "vitest": "^0.4.1"
  },
  "packageManager": "yarn@3.1.1"
}
