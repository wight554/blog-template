{
  "name": "blog-template",
  "version": "0.0.0",
  "scripts": {
    "cleanup": "rimraf dist",
    "dev": "tsnd --respawn -r tsconfig-paths/register server/server",
    "prod": "node ./dist/server/server.js",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json && vite build",
    "build": "yarn cleanup && yarn build:client && yarn build:server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint \"{src,server,test}/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\" \"test/**/*.{ts,tsx}\"",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.2",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-cookie": "^5.6.0",
    "fastify-static": "^4.6.1",
    "mongoose": "^6.2.7",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preact": "^10.6.6",
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest",
    "recoil": "^0.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.2",
    "@preact/preset-vite": "^2.1.7",
    "@testing-library/preact": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/node": "^17.0.22",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "c8": "^7.11.0",
    "eslint": "^8.11.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.50.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-checker": "^0.4.3",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.7.7"
  },
  "packageManager": "yarn@3.1.1"
}
